  "bugs": {
    "url": "https://github.com/GitKou/Nut/issues"
  },
  "homepage": "https://github.com/GitKou/Nut#readme",
  "description": "",
  "scripts": {
    "build": "father build",
    "docDev": "father doc dev",
    "docBuild": "father doc build",
    "docDeploy": "father doc deploy",
    "doc": "father doc",
    "bootstrap": "lerna bootstrap",
    "publish": "lerna publish",
    "lint": "eslint --cache --ext .js,.jsx,.ts,.tsx  --fix --format=pretty ./packages && npm run lint:prettier && npm run check-deps",
    "lint:prettier": "npm run prettier &&  git diff && prettier --version && prettier --check \"packages/**/**.{js,jsx,tsx,ts,less,md,json}\" --end-of-line auto",
    "prettier": "prettier --write \"**/**.{js,jsx,tsx,ts,less,md,json}\""
  },
  "devDependencies": {
    "eslint": "^8.2.0",
    "father": "^2.30.10",
    "lerna": "^4.0.0",
    "lint-staged": "^11.2.6",
    "pretty-quick": "^3.1.1",
    "stylelint": "^14.0.1"
  },
  "name": "lc-nut",
  "dependencies": {
    "@umijs/fabric": "^2.8.1",
    "antd": "^4.16.13"
  },
  "version": "0.0.1",
  "gitHooks": {
    "pre-commit": "pretty-quick --staged",
    "commit-msg": "fabric verify-commit"
  },
  "sideEffects": false,
  "browserslist": [
    "last 2 versions",
    "Firefox ESR",
    "> 1%",
    "ie >= 11"
  ]
}
